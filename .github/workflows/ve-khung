#define MINX 2
#define MINY 2
#define MAXX 20
#define MAXY 15

void VeKhung() {
    for (int i = MINX; i <= MAXX; i++) {
        for (int j = MINY; j <= MAXY; j++) {
            if (i == MINX || i == MAXX || j == MINY || j == MAXY) {
                gotoxy(i, j);
                cout << "+";
            }
        }
    }
}
